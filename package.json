{
  "name": "leaflet-regular-grid-cluster",
  "version": "0.1.1",
  "description": "leaflet plugin that implements custom grid-style visualisation and clustering method",
  "main": "dist/leaflet-regulargridcluster.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adammertel/Leaflet.RegularGridCluster.git"
  },
  "keywords": [
    "leaflet",
    "maps",
    "grid",
    "cluster",
    "visualisation"
  ],
  "author": "Adam Mertel, University of Vienna",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adammertel/Leaflet.RegularGridCluster/issues"
  },
  "homepage": "https://github.com/adammertel/Leaflet.RegularGridCluster#readme",
  "devDependencies": {
    "@comandeer/babel-plugin-banner": "^1.0.0",
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-remove-console": "^6.8.3",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.24.1",
    "bower": "^1.8.0",
    "copyfiles": "^1.0.0",
    "jshint": "^2.9.3",
    "npm-run-all": "^4.0.2"
  },
  "scripts": {
    "jshint": "jshint src",
    "babel": "babel src --out-file dist/leaflet-regulargridcluster.src.js --no-coments",
    "minify-js": "babel --plugins transform-remove-console dist/leaflet-regulargridcluster.src.js --out-file dist/leaflet-regulargridcluster.js --minified",
    "onchange-js": "onchange src/**/*.js -- npm run build",
    "onchange-css": "onchange src/**/*.css -- npm run move-css",
    "move-css": "copyfiles -f src/leaflet-regulargridcluster.css dist",
    "build": "run-s jshint babel minify-js move-css",
    "start": "run-p onchange-js build"
  },
  "dependencies": {
    "leaflet": "^1.0.3"
  }
}
